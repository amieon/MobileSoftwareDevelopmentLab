/*页面容器样式*/
.container{
  height:100vh;
  color:#E64340;
  font-weight:bold;
  display: flex;
  flex-direction: column;
  align-items:center;
  justify-content:space-evenly;
}
  /*顶端标题样式*/
.title {
  font-size:18pt;
}
/*关卡列表区域*/
.levelBox{
width:100%;
}
